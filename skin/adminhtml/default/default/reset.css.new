#sales_order_view_tabs_my_custom_tab2_content .tooltip{position:absolute; width: 450px; border:1px solid #ccc; background:#fff; top:0px; left:50%; margin-left:-250px; height: 430px; box-shadow:0 0 10px #000000;}
