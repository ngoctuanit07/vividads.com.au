.edittasktab tr.editdel{text-align:center;}
.edittasktab td.label{padding: 0px 1px;}
.eventcalendar-adminhtml-eventcalendar-index .scalable{margin-right:6px;}

.edittodotab{width:936px !important;}
.edittodotab tr{line-height:28px;}
.edittodotab td.label{padding: 0px 1px;}

.addtask1 {width:990px !important; height: 454px !important; position: fixed !important; top: 50px;left: 20%;background-color: white;z-index: 999;box-shadow: 0 2px 4px rgba(0,0,0,.2); background: white;border: 1px solid #CCC;}
.addtask1 .addclose { position: absolute !important;  left: 960px; top: 14px; cursor: pointer; font-size: 18px;}
.addtask1 tr.editdel{text-align:center;}


.addtaskcls{width: 780px !important; margin: 0px auto;}
.addtaskcls tr{line-height:28px;}
.addtaskcls tr.addtasktrsave{text-align:center;}


.addtask1 .addtodotab {width:800px !important; margin: 0px auto;}
.addtask1 .addtodotab tr{line-height:32px;}
.addtodotab .ot_created_date{width:70px;}
.edittodotab .ot_created_date{width:70px;}

