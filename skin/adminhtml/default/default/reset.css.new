.tooltip .allcomment{width: 405px; overflow-y:scroll; margin: 20px; height: 180px; border: 1px solid #dfdfdf; background:#fafafa; padding:0px;}

.tooltip .allcomment .comtotal:nth-child(odd){background:#f2f2f2; line-height: 22px; padding:5px 0 5px 5px;}
.tooltip .allcomment .comtotal:nth-child(even){background:#fff; line-height: 22px; padding:5px 0 5px 5px;}


.tooltip #dvFile{width: 405px; overflow:hidden; margin:5px 3px;}
.tooltip #dvFile .file_class{width:100%; overflow:hidden; margin-bottom:6px;}
.tooltip #dvFile .item_comment textarea{height:60px; max-width:390px; min-width:390px; resize: none;}
.tooltip .fromtool td.clssubmit{text-align: center; display:block;line-height: 25px;}




