
* {
    margin: 0;
    padding: 0;
}
body {
    background: none repeat scroll 0 0 #E6E6E6;
    color: #2F2F2F;
    font: 12px/1.5em Arial,Helvetica,sans-serif;
}
a {
    color: #EA7601;
    text-decoration: underline;
}
a:hover {
    color: #EA7601;
    text-decoration: underline;
}
a img {
    border: 0 none;
}
:focus {
    outline: 0 none;
}
h1, h2, h3, h4 {
    line-height: 1.4em;
    margin-bottom: 0.5em;
}
h2 {
    font-size: 1.7em;
}
h3 {
    color: #253033;
    font-size: 1.25em;
    margin-bottom: 0.5em;
}
h4 {
    color: #494848;
    font-size: 1.05em;
    margin-bottom: 0.6em;
}
h5 {
    font-size: 1.05em;
}
h6 {
    font-size: 1em;
}
h1 a, h1 a:hover, h2 a, h2 a:hover, h3 a, h3 a:hover, h4 a, h4 a:hover {
    font-weight: normal;
}
th {
    padding: 0;
    text-align: left;
    vertical-align: top;
}
td {
    padding: 0;
    vertical-align: top;
}
p, address {
    margin-bottom: 0.5em;
}
address {
    font-style: normal;
}
cite {
    font-size: 10px;
    font-style: normal;
}
q:before, q:after {
    content: "";
}
form {
    display: inline;
}
fieldset {
    border: 0 none;
}
legend {
    display: block !important;
    height: 0;
    line-height: 0;
    margin: 0;
    overflow: hidden;
    padding: 0;
    visibility: hidden;
    width: 0;
}
label {
    color: #333333;
}
input, select {
    vertical-align: middle;
}
textarea {
    overflow: auto;
}
ul, ol {
    list-style: none outside none;
}
dt {
    display: block;
}
small {
    font-size: 0.9em;
}
big {
    font-size: 1.25em;
}
.event_calendar {
    background: none repeat scroll 0 0 #3D3C3D;
    position: relative;
}
.event_calendar tr td.day_header {
    background: none repeat scroll 0 0 transparent;
    color: #FFFFFF;
    font-weight: bold;
    margin: 0;
    padding: 0;
    text-align: center;
}
.event_calendar tr td.week_header {
    background: none repeat scroll 0 0 #F1F1F1;
    color: #333333;
    font-weight: bold;
    margin: 0;
    padding: 0;
    text-align: center;
}
.event_calendar tr td.event_day {
    background: none repeat scroll 0 0 #ECB400;
    color: #FFFFFF;
    cursor: pointer;
    font-size: 14px;
    font-style: italic;
    font-weight: bold;
    margin: 0;
    padding: 0;
    text-align: center;
}
.event_calendar caption {
    background: none repeat scroll 0 0 #000000;
    color: #FFFFFF;
    font-size: 18px;
    font-weight: bold;
    padding: 10px;
}
.lft_arr, .right_arr {
    color: #FFFFFF;
    font-size: 14px;
    font-weight: bold;
    padding: 10px;
    position: absolute;
    text-decoration: none !important;
    top: 2px;
}
.lft_arr {
    left: 5px;
}
.right_arr {
    right: 5px;
}
.event_calendar .outer, .sc_calendar .outer {
    border: 0 none !important;
    box-shadow: 0 0 10px 2px #333333;
    color: #000000;
    cursor: default !important;
    left: 50% !important;
    margin-left: -200px !important;
    padding: 20px;
    text-align: left;
}
.event_calendar .inner {
    position: static !important;
}
.event_calendar .inner > div {
    clear: both;
    font-style: normal;
    font-weight: normal;
    margin: 0 0 5px;
}
.event_calendar .inner > div span {
    clear: both;
    float: none !important;
}
.event_calendar .outer .cross, .sc_calendar .cross {
    font-style: normal;
    left: 94% !important;
}
.sc_calendar .inner {
    position: static !important;
}
.event_calendar tr td.event_day.activate {
    background: none repeat scroll 0 0 #EC5E00;
}
.sc_calendar {
   border-left: 1px solid #B3B3B3; 
   margin-top: 0 !important;
}
.sc_calendar caption {
    color: #000000;
    font-size: 14px;
    padding: 0;
    text-align: center;
    font-weight: bold;
}
.sc_calendar tr th {
    background: none;
}
.sc_calendar tr th {
    border-bottom: 1px solid #B3B3B3;
    padding: 10px 0 0 4px;
    text-align:left;
    width: 10%;
    font-weight:normal;
}
.sc_calendar tr td {
    border-bottom: 1px solid #ddd;
    border-right: 1px solid #ddd;
    width: 10%;
}
.sc_calendar tr td.sc_day {
    height: 150px;
    position: relative;
    text-align: center;
}
.sc_calendar tr td.sc_day.active_view {
    background: none repeat scroll 0 0 #F1F1F1;
}
span.ddy.holiday {
    background: none repeat scroll 0 0 red;
}
span.ddy {
    color: #000;
    display: block;
    position: absolute;
    top: 1px;
    width: 20px;
    cursor:pointer;   
}
span.ddy.activated_day {
    font-weight:bold;
    cursor:pointer;
}
.arr_l, .arr_r {
    font-size: 13px;
}
.tot_ord_det {
    font-size: 14px;
    padding: 20px;
    text-align: left;
}
.tot_ord_det {
    float: left;
    font-size: 14px;
    padding: 20px;
    text-align: left;
    width: 179px;
}
.holiday_lnk {
    float: left;
    font-size: 14px;
    padding: 20px;
    text-align: left;
    width: 170px;
}
.tot_ord_det .ord_lnk span {
    padding-right: 10px;
}
.all_data {
    clear: none !important;
    float: left;
    width: 200px;
}
.holi_lnk {
    clear: none !important;
    float: left;
    width: 170px;
}
#holiday_list ul.listing li {
    color: #000000;
    font-weight: bold;
    margin: 2px 0;
    padding: 5px;
}
#holiday_list ul.listing li.odd {
    background: none repeat scroll 0 0 #FBEBC9;
}
#holiday_list ul.listing li.even {
    background: none repeat scroll 0 0 #FFF7E6;
}
#holiday_list {
    margin: 10px 0 0;
}
.search_date, .search_country {
    background: none repeat scroll 0 0 #F1F1F1;
    border: 1px solid #CCCCCC;
    padding: 5px;
}
.search_country {
    margin: 0 0 10px;
}
.file_class {
    float: left;
    width: 190px;
}
.item_class {
    float: left;
}
.removeitem, .addanother, .submit-img {
    background: none repeat scroll 0 0 #ED6502;
    border: 1px solid #A04300;
    color: white;
    font-weight: bold;
    margin-left: 5px;
    padding: 1px 10px;
}
.submitclass {
    padding: 10px 20px;
}
.total_proof {
    padding: 0 20px;
    width: 70%;
}
.total_proof tr td {
    padding: 3px 0 !important;
}
.lft_arr_year {
    color: #FFFFFF;
    font-size: 14px;
    font-weight: bold;
    left: 60px;
    position: absolute;
    text-decoration: none;
    top: 11px;
}
.right_arr_year {
    color: #FFFFFF;
    font-size: 14px;
    font-weight: bold;
    position: absolute;
    right: 60px;
    text-decoration: none;
    top: 11px;
}
.lft_arr_year:hover, .right_arr_year:hover {
    text-decoration: none;
}
.outer.taskcl {
    height: 300px;
    overflow-x: hidden;
    overflow-y: scroll;
    top: 50px;
}
.outer.taskcl .tot_ord_det {
    padding: 20px 0 0;
}

