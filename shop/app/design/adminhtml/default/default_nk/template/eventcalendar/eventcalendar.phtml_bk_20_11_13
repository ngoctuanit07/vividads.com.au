    $temptableSaleOrder=Mage::getSingleton('core/resource')->getTableName('sales_flat_order');
    $sqlSaleOrder="SELECT entity_id,created_at FROM ".$temptableSaleOrder;//." WHERE is_event_set_to_google_calendar='0'";
