<?xml version="1.0"?>
<layout version="0.1.0">    
    <default>        
        <reference name="head">           
            <!--action method="addJs" ifconfig="brandlogo/general/brandlogoenabled"><script>brandlogo/jquery-1.8.3.min.js</script></action>                            
            <action method="addJs" ifconfig="brandlogo/general/brandlogoenabled"><script>brandlogo/noconflict.js</script></action-->                                         
            <action method="addCss" ifconfig="brandlogo/general/brandlogoenabled">
                <stylesheet>css/brandlogo/carousel.css</stylesheet>
            </action>
        </reference>   
    </default>
    
    <brandlogo_index_index>
        <reference name="content">
            <block type="brandlogo/brandlogo" name="brandlogo" template="brandlogo/brandlogo.phtml" /> 
        </reference>
    </brandlogo_index_index>
    <brandlogo_index_list>
        <reference name="content">
            <block type="brandlogo/list" name="list" template="brandlogo/list.phtml" /> 
        </reference>
    </brandlogo_index_list>
</layout> 
