	function isNumeric(value) {
    var bool = isNaN(value);
   
    return bool;
	} 
	
	/*validate card numbers*/
	function validateCard(inputtxt,cardType)  
		{  
		  /* validating card numbers */
		  
		  var amec_cardno = /^(?:3[47][0-9]{13})$/;  
		  var visa_cardno = /^(?:4[0-9]{12}(?:[0-9]{3})?)$/; 
		  var master_cardno = /^(?:5[1-5][0-9]{14})$/; 
		//  var discover_cardno = /^(?:6(?:011|5[0-9][0-9])[0-9]{12})$/; 
		//  var jcb_cardno = /^(?:(?:2131|1800|35\d{3})\d{11})$/; 
		  
		/* American Express Validation */
		if(!inputtxt.value.match(amec_cardno) && cardType.value=='AE')  
			{  
			 	alert("Not a valid Amercican Express credit card number!");  
				return false; 
			}  
		/* VISA card Validation */
		if(!inputtxt.value.match(visa_cardno) && cardType.value=='VI')  
			{  
			  
				alert("Not a valid Visa credit card number!");  
				return false; 
			} 
		/* Master card Validation */
		if(!inputtxt.value.match(master_cardno) && cardType.value=='MC')  
			{  
			 
				alert("Not a valid Master credit card number!");  
				return false; 
			} 	
			
			return true; 	
	}  
	
	
	
