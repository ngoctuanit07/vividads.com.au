	  	  var strURL = 'headlines.php';

 		  var req = getXMLHTTP();		

		  if(req)

		  {

				req.onreadystatechange = function()

				{

					if (req.readyState == 4)

					{

						if (req.status == 200)

						{

							if(req.responseText !== 0)

							{

 								document.getElementById('latestblogcomment').innerHTML = req.responseText;

 							}

						}

					}

				}

				req.open("GET", strURL, true);

				req.send(null);

			}	

	  }

	 </script> 

