<config>
    <modules>
        <MageWorx_XSitemap>
            <version>3.1.6</version>
        </MageWorx_XSitemap>
    </modules>
    <frontend>
        
        <routers>
            <xsitemap>
                <use>standard</use>
                <args>
                    <module>MageWorx_XSitemap</module>
                    <frontName>sitemap</frontName>
                </args>
            </xsitemap>
        </routers>
        <layout>
            <updates>
                <xsitemap>
                    <file>xsitemap.xml</file>
                </xsitemap>
            </updates>
        </layout>
    </frontend>
  
</config>